{
  "name": "one-typescript",
  "version": "0.0.6",
  "description": "Porting of One package to typescript",
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "test",
    "clean": "rimraf lib dist coverage",
    "build:js": "webpack --config webpack.config.js",
    "build:js:min": "webpack --config webpack.config.production.js",
    "build:lib": "tsc --rootDir src --outDir lib",
    "build": "npm run build:lib && npm run build:js && npm run build:js:min",
    "tt": "webpack --config webpack.st.config.js",
    "prepub": "npm run clean && npm run build"
  },
  "author": "Dan Maier",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "@types/node": "^7.0.5",
    "@types/sinon": "^1.16.32",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-babel": "^3.0.3",
    "path": "^0.12.7",
    "rimraf": "^2.5.4",
    "sinon": "git+https://github.com/sinonjs/sinon.git",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^1.2.2",
    "typescript": "^2.1.6",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {}
}
